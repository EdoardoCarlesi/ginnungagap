include ../Makefile.config

.PHONY: all clean tests dist-clean

all:
	$(MAKE) -C makeSiloRoot all

clean:
	$(MAKE) -C makeSiloRoot clean

tests:
	$(MAKE) -C makeSiloRoot tests

dist-clean:
	$(MAKE) -C makeSiloRoot dist-clean
